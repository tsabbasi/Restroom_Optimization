function result = expon(lambda, trials)

% avg number of patrons during each half time
num_women = 197;
% RATES lambda per patron type
lamda_womens_br = num_women/90;

% Arrival Times
w_arrival_times = expon(lamda_womens_br, 115);

% Cummulative arrival times on a timeline
arrival_times = [];

% mu is 1/lambda
ppl = 1
while (max(cumsum(arrival_times)) <= 90)
    arrival_times(ppl) = exprnd(1/lambda);
    ppl = ppl + 1;
end

result = arrival_times;
end


